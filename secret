    local btnPlus = Instance.new("TextButton")
    btnPlus.Size = UDim2.new(0, 44, 0, 28)
    btnPlus.Position = UDim2.new(0.78, 0, 0.15, 0)
    btnPlus.BackgroundColor3 = Color3.fromRGB(58,58,64)
