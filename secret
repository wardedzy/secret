            flyToggleBtn.BackgroundColor3 = Color3.fromRGB(78,150,83)
            flyToggleBtn.Text = "ON"
            enableFly()
        else
            flyToggleBtn.BackgroundColor3 = Color3.fromRGB(190,80,80)
            flyToggleBtn.Text = "OFF"
            disableFly()
        end
    end)

